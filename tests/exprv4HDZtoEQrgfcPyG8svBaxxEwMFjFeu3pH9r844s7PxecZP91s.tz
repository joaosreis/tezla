{ parameter (or (unit %retrieveTotalSupply) (nat %setTotalSupply)) ;
  storage (pair (address %fa12) (nat %totalSupply)) ;
  code { DUP ;
         CDR ;
         DIG 1 ;
         DUP ;
         DUG 2 ;
         CAR ;
         IF_LEFT
           { NIL operation ;
             DUP ;
             DIG 3 ;
             DUP ;
             DUG 4 ;
             CAR ;
             CONTRACT %getTotalSupply (contract nat) ;
             IF_NONE { PUSH string "error self" ; FAILWITH } { DUP ; DIP { DROP } } ;
             PUSH mutez 0 ;
             SELF ;
             ADDRESS ;
             CONTRACT %setTotalSupply nat ;
             IF_NONE { PUSH string "error self" ; FAILWITH } { DUP ; DIP { DROP } } ;
             TRANSFER_TOKENS ;
             CONS ;
             DIG 3 ;
             DUP ;
             DUG 4 ;
             DIG 1 ;
             DUP ;
             DUG 2 ;
             PAIR ;
             DIP { DROP 3 } }
           { DIG 1 ;
             DUP ;
             DUG 2 ;
             DIG 1 ;
             DUP ;
             DUG 2 ;
             PAIR ;
             DUP ;
             CDR ;
             DUP ;
             CAR ;
             SENDER ;
             COMPARE ;
             EQ ;
             NOT ;
             IF { PUSH string "invalid caller" ; FAILWITH } { PUSH unit Unit } ;
             DIG 1 ;
             DUP ;
             DUG 2 ;
             DIG 3 ;
             DUP ;
             DUG 4 ;
             CAR ;
             SWAP ;
             CAR ;
             PAIR ;
             NIL operation ;
             PAIR ;
             DIP { DROP 4 } } ;
         DIP { DROP 2 } } }
