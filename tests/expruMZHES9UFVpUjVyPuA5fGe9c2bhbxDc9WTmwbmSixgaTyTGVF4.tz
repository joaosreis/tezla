             DUP ;
             DUG 2 ;
             CAR ;
             PACK ;
             SENDER ;
             PACK ;
             COMPARE ;
             EQ ;
             IF {} { PUSH string "Unauthorized Action. You are not manager." ; FAILWITH } ;
             SWAP ;
             DUP ;
             CAR ;
             SWAP ;
             CDR ;
             PUSH bool True ;
             DIG 3 ;
             UPDATE ;
             SWAP ;
             PAIR }
           { SWAP ;
             DUP ;
             DUG 2 ;
             CAR ;
             PACK ;
             SENDER ;
             PACK ;
             COMPARE ;
             EQ ;
             IF {} { PUSH string "Unauthorized Action. You are not manager." ; FAILWITH } ;
