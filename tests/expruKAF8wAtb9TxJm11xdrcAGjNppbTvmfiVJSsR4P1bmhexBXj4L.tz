{ parameter
    (pair (pair nat (or (pair nat (contract nat)) (pair nat (list key)))) (list (option signature))) ;
  storage (pair nat (pair nat (list key))) ;
  code { DUP ;
         CAR ;
         DIP { CDR } ;
         PUSH mutez 0 ;
         AMOUNT ;
         COMPARE ;
         EQ ;
         IF {}
