             DIP { DUP ;
                   CAR ;
                   DIP { CDR } ;
                   DIP { DIP { NIL operation } ; PUSH mutez 0 } ;
                   TRANSFER_TOKENS ;
                   CONS } ;
             SWAP }
           { DIP { CAR } ; SWAP ; PAIR ; NIL operation } ;
         PAIR } }
