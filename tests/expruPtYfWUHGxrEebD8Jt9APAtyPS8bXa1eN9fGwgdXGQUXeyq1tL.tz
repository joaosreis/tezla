{ parameter (or (unit %deposit) (unit %withdraw)) ;
  storage mutez ;
  code { NIL operation ;
         DIG 1 ;
         DUP ;
         DUG 2 ;
         CDR ;
         DIG 2 ;
         DUP ;
         DUG 3 ;
         CAR ;
         IF_LEFT
           { PUSH mutez 0 ;
             AMOUNT ;
             COMPARE ;
             EQ ;
             IF { DIG 1 ; DUP ; DUG 2 }
                { DIG 1 ;
                  DUP ;
                  DUG 2 ;
                  AMOUNT ;
                  DIG 3 ;
                  DUP ;
                  DUG 4 ;
                  ADD ;
                  DIP { DROP } ;
                  DIG 2 ;
                  DUP ;
                  DUG 3 ;
                  DIG 1 ;
                  DUP ;
                  DUG 2 ;
                  DIP { DROP 2 } } ;
             DUP ;
             DIG 4 ;
             DUP ;
             DUG 5 ;
             PAIR ;
             DIP { DROP 2 } }
           { AMOUNT ;
             NIL operation ;
             DIG 3 ;
             DUP ;
             DUG 4 ;
             DIG 2 ;
             DUP ;
             DUG 3 ;
             COMPARE ;
             GT ;
             IF { DIG 3 ; DUP ; DUG 4 }
                { DIG 3 ;
                  DUP ;
                  DUG 4 ;
                  DIG 2 ;
                  DUP ;
                  DUG 3 ;
                  DIG 5 ;
                  DUP ;
                  DUG 6 ;
                  SUB ;
                  DIP { DROP } ;
                  DIG 4 ;
                  DUP ;
                  DUG 5 ;
                  DIG 1 ;
                  DUP ;
                  DUG 2 ;
                  DIP { DROP 2 } } ;
             DUP ;
             DIG 6 ;
             DUP ;
             DUG 7 ;
             PAIR ;
             DIP { DROP 4 } } ;
         DIP { DROP 3 } } }
