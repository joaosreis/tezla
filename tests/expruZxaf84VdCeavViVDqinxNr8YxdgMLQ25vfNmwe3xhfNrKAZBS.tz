         CDR ;
         SWAP ;
         CAR ;
         IF_LEFT
           { DROP ;
             DUP ;
             CAR ;
             CAR ;
             SENDER ;
             COMPARE ;
             EQ ;
             IF {}
                { PUSH string "WrongCondition: self.data.bot_manager == sp.sender" ;
                  FAILWITH } }
           { SENDER ;
             DIG 2 ;
             DUP ;
             DUG 3 ;
             CAR ;
             CAR ;
             COMPARE ;
             EQ ;
             IF {} { PUSH string "Error: non manager call" ; FAILWITH } ;
             PUSH int 1 ;
             DIG 2 ;
             DUP ;
             DUG 3 ;
             CDR ;
             CDR ;
             CAR ;
             COMPARE ;
             GE ;
             IF { SWAP ;
                  DUP ;
                  CAR ;
                  SWAP ;
                  CDR ;
                  DUP ;
                  CAR ;
                  SWAP ;
                  CDR ;
                  DUP ;
                  CDR ;
                  SWAP ;
                  CAR ;
                  PUSH int 1 ;
                  SWAP ;
                  SUB ;
                  PAIR ;
                  SWAP ;
                  PAIR ;
                  SWAP ;
                  PAIR ;
                  DUP ;
                  CAR ;
                  SWAP ;
                  CDR ;
                  DUP ;
                  CAR ;
                  SWAP ;
                  CDR ;
                  DUP ;
                  CAR ;
                  SWAP ;
                  CDR ;
                  DUP ;
                  DIG 5 ;
                  DUP ;
                  DUG 2 ;
                  GET ;
                  IF_NONE { PUSH int 31 ; FAILWITH } {} ;
                  PUSH nat 1 ;
                  ADD ;
                  SOME ;
                  SWAP ;
                  UPDATE ;
                  SWAP ;
                  PAIR ;
                  SWAP ;
                  PAIR ;
                  SWAP ;
                  PAIR }
                { PUSH string "Error: you ran out of bullets! Please buy more!" ; FAILWITH } } ;
         NIL operation ;
         PAIR } }
