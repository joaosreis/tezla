{ storage
    (pair (pair (address %bot_manager) (bool %is_alive))
          (pair (string %name)
                (pair (int %plasma_bullet_count) (map %record_alien_kills string nat)))) ;
  parameter (or (unit %doNothin) (string %shoot_alien)) ;
