{ parameter (or (pair %add_photo (string %caption) (string %content)) (int %tip_photo)) ;
  storage
    (pair (int %photo_count)
          (map %photos
             int
             (pair (pair (string %caption) (string %content))
                   (pair (int %id) (pair (address %owner) (mutez %tip_amount)))))) ;
  code { DUP ;
         CDR ;
         SWAP ;
         CAR ;
         IF_LEFT
           { SWAP ;
             DUP ;
             DUG 2 ;
             DUP ;
             CAR ;
             SWAP ;
             CDR ;
             DIG 3 ;
             DUP ;
             DUG 4 ;
             CAR ;
             PUSH mutez 0 ;
             SENDER ;
             PAIR %owner %tip_amount ;
             DIG 5 ;
             CAR ;
             PAIR %id ;
             DIG 4 ;
             DUP ;
             DUG 5 ;
             CDR ;
             DIG 5 ;
             CAR ;
             PAIR %caption %content ;
             PAIR ;
             SOME ;
             SWAP ;
             UPDATE ;
             SWAP ;
             PAIR ;
             DUP ;
             CDR ;
             SWAP ;
             CAR ;
             PUSH int 1 ;
             ADD ;
             PAIR }
           { PUSH mutez 0 ;
             AMOUNT ;
             COMPARE ;
             GT ;
             IF {} { PUSH string "WrongCondition: sp.amount > sp.tez(0)" ; FAILWITH } ;
             DUP ;
             PUSH int 0 ;
             SWAP ;
             COMPARE ;
             GE ;
             IF {} { PUSH string "WrongCondition: params.photo_id >= 0" ; FAILWITH } ;
             SWAP ;
             DUP ;
             DUG 2 ;
             CAR ;
             SWAP ;
             DUP ;
             DUG 2 ;
             COMPARE ;
             LE ;
             IF {}
                { PUSH string "WrongCondition: params.photo_id <= self.data.photo_count" ;
                  FAILWITH } ;
             SWAP ;
             DUP ;
             CAR ;
             SWAP ;
             CDR ;
             DUP ;
             DIG 3 ;
             DUP ;
             DUG 2 ;
             GET ;
             { IF_NONE { PUSH int 25 ; FAILWITH } {} } ;
             DUP ;
             CAR ;
             SWAP ;
             CDR ;
             DUP ;
             CAR ;
             SWAP ;
             CDR ;
             DUP ;
             CAR ;
             SWAP ;
             CDR ;
             AMOUNT ;
             ADD ;
             SWAP ;
             PAIR ;
             SWAP ;
             PAIR ;
             SWAP ;
             PAIR ;
             SOME ;
             SWAP ;
             UPDATE ;
             SWAP ;
             PAIR } ;
         NIL operation ;
         PAIR } }
