{ storage
    (pair (big_map %credentials address string)
          (pair (string %modulus) (big_map %vote_commitments address string))) ;
  parameter (or (string %add_credential) (string %vote)) ;
  code { DUP ;
         CDR ;
         SWAP ;
         CAR ;
         IF_LEFT
           { { SWAP ;
               DUP ;
               DUG 2 ;
               DUP ;
               CDR ;
               SWAP ;
               CAR ;
               DIG 2 ;
               DUP ;
               DUG 3 ;
               SOME ;
               SENDER ;
               UPDATE ;
               PAIR ;
               DUG 2 ;
               DROP 2 } }
           { { SWAP ;
               DUP ;
               DUG 2 ;
               CDR ;
               CDR ;
               SENDER ;
               MEM ;
               IF { { SWAP ;
                      DUP ;
                      DUG 2 ;
                      DUP ;
                      CAR ;
                      SWAP ;
                      CDR ;
                      DUP ;
                      CAR ;
                      SWAP ;
                      CDR ;
                      DIG 3 ;
                      DUP ;
                      DUG 4 ;
                      DIG 5 ;
                      DUP ;
                      DUG 6 ;
                      CDR ;
                      CDR ;
                      SENDER ;
                      GET ;
                      IF_NONE { { PUSH string "Get-item:-1" ; FAILWITH } } {} ;
                      CONCAT ;
                      SOME ;
                      SENDER ;
                      UPDATE ;
                      SWAP ;
                      PAIR ;
                      SWAP ;
                      PAIR ;
                      DUG 2 ;
                      SWAP ;
                      DROP } }
                  { { SWAP ;
                      DUP ;
                      DUG 2 ;
                      DUP ;
                      CAR ;
                      SWAP ;
                      CDR ;
                      DUP ;
                      CAR ;
                      SWAP ;
                      CDR ;
                      DIG 3 ;
                      DUP ;
                      DUG 4 ;
                      SOME ;
                      SENDER ;
                      UPDATE ;
                      SWAP ;
                      PAIR ;
                      SWAP ;
                      PAIR ;
                      DUG 2 ;
                      SWAP ;
                      DROP } } ;
               DROP } } ;
         NIL operation ;
         PAIR } }
