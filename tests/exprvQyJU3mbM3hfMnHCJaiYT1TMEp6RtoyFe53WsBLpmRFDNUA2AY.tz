{ parameter (or (address %buy) (mutez %withdraw)) ;
  storage
    (pair (pair (pair (address %fa2contractaddress) (address %fa2owneraddress))
                (pair (mutez %initaltacoz) (address %lastorder)))
          (address %owner)) ;
  code { UNPAIR ;
         IF_LEFT
           { PUSH mutez 1000000 ;
             AMOUNT ;
             DUP 4 ;
             CAR ;
             CDR ;
             CAR ;
             SUB ;
             COMPARE ;
             LT ;
             IF { DROP 2 ; PUSH string "All Tacoz sold." ; FAILWITH }
                { PUSH nat 120000 ;
                  PUSH nat 1000000000000000000 ;
                  PUSH mutez 1000000 ;
                  AMOUNT ;
                  EDIV ;
                  IF_NONE { PUSH string "DIV by 0" ; FAILWITH } {} ;
                  CAR ;
                  MUL ;
                  MUL ;
                  DUP 3 ;
                  CAR ;
                  CAR ;
                  CAR ;
                  CONTRACT %transfer
                    (list (pair (address %from_)
                                (list %txs (pair (address %to_) (pair (nat %token_id) (nat %amount)))))) ;
                  IF_NONE { PUSH string "Contract not found :/ ." ; FAILWITH } {} ;
                  NIL (pair address (pair nat nat)) ;
                  DUP 3 ;
                  PUSH nat 0 ;
                  PAIR ;
                  SENDER ;
                  PAIR ;
                  CONS ;
                  DUP 5 ;
                  CAR ;
                  CAR ;
                  CDR ;
                  PAIR ;
                  PUSH nat 100 ;
                  PUSH nat 5 ;
                  DIG 4 ;
                  MUL ;
                  EDIV ;
                  IF_NONE { PUSH string "DIV by 0" ; FAILWITH } {} ;
                  CAR ;
                  SENDER ;
                  DUP 5 ;
                  COMPARE ;
                  NEQ ;
                  IF { NIL operation ;
                       DUP 4 ;
                       PUSH mutez 0 ;
                       NIL (pair address (list (pair address (pair nat nat)))) ;
                       NIL (pair address (pair nat nat)) ;
                       DIG 5 ;
                       PUSH nat 0 ;
                       PAIR ;
                       DUP 9 ;
                       PAIR ;
                       CONS ;
                       DUP 9 ;
                       CAR ;
                       CAR ;
                       CDR ;
                       PAIR ;
                       CONS ;
                       TRANSFER_TOKENS ;
                       CONS ;
                       DIG 2 ;
                       PUSH mutez 0 ;
                       NIL (pair address (list (pair address (pair nat nat)))) ;
                       DIG 4 ;
                       CONS ;
                       TRANSFER_TOKENS ;
                       CONS ;
                       DUP 3 ;
                       CDR ;
                       DUP 4 ;
                       CAR ;
                       CDR ;
                       CDR ;
                       AMOUNT ;
                       DUP 6 ;
                       CAR ;
                       CDR ;
                       CAR ;
                       SUB ;
                       PAIR ;
                       DIG 4 ;
                       CAR ;
                       CAR ;
                       PAIR ;
                       PAIR ;
                       DUP ;
                       CDR ;
                       DIG 3 ;
                       DUP 3 ;
                       CAR ;
                       CDR ;
                       CAR ;
                       PAIR ;
                       DIG 2 ;
                       CAR ;
                       CAR ;
                       PAIR ;
                       PAIR ;
                       SWAP ;
                       PAIR }
                     { DROP ;
                       NIL operation ;
                       DIG 2 ;
                       PUSH mutez 0 ;
                       NIL (pair address (list (pair address (pair nat nat)))) ;
                       DIG 4 ;
                       CONS ;
                       TRANSFER_TOKENS ;
                       CONS ;
                       DUP 3 ;
                       CDR ;
                       DUP 4 ;
                       CAR ;
                       CDR ;
                       CDR ;
                       AMOUNT ;
                       DUP 6 ;
                       CAR ;
                       CDR ;
                       CAR ;
                       SUB ;
                       PAIR ;
                       DIG 4 ;
                       CAR ;
                       CAR ;
                       PAIR ;
                       PAIR ;
                       DUP ;
                       CDR ;
                       DIG 3 ;
                       DUP 3 ;
                       CAR ;
                       CDR ;
                       CAR ;
                       PAIR ;
                       DIG 2 ;
                       CAR ;
                       CAR ;
                       PAIR ;
                       PAIR ;
                       SWAP ;
                       PAIR } } }
           { SENDER ;
             DUP 3 ;
             CAR ;
             CAR ;
             CDR ;
             COMPARE ;
             NEQ ;
             IF { DROP 2 ; PUSH string "You are not the contract owner." ; FAILWITH }
                { SWAP ;
                  DUP ;
                  DUG 2 ;
                  CDR ;
                  CONTRACT unit ;
                  IF_NONE { PUSH string "Contract not found." ; FAILWITH } {} ;
                  SWAP ;
                  UNIT ;
                  TRANSFER_TOKENS ;
                  SWAP ;
                  NIL operation ;
                  DIG 2 ;
                  CONS ;
                  PAIR } } } }
