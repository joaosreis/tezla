{ parameter (or (unit %deposit) (mutez %withdraw)) ;
  storage (pair (big_map %tezs address mutez) (nat %x)) ;
  code { { { DUP ; CAR ; DIP { CDR } } } ;
         IF_LEFT
           { DROP ;
             PUSH mutez 1 ;
             AMOUNT ;
             COMPARE ;
             LT ;
             IF { PUSH string "AvocadoExchange | Tezos amount should be greater than 0" ;
                  FAILWITH }
                {} ;
             DUP ;
             CAR ;
             SENDER ;
             GET ;
             IF_NONE { PUSH mutez 0 } {} ;
             SWAP ;
             DUP ;
             DUG 2 ;
             CDR ;
             DIG 2 ;
             CAR ;
             AMOUNT ;
             DIG 3 ;
             ADD ;
             SOME ;
             SENDER ;
             UPDATE ;
             PAIR ;
             NIL operation ;
             PAIR }
           { PUSH mutez 1 ;
             SWAP ;
             DUP ;
             DUG 2 ;
             COMPARE ;
             LT ;
             IF { PUSH string "AvocadoExchange | Tezos amount should be greater than 0" ;
                  FAILWITH }
                {} ;
             SWAP ;
             DUP ;
             DUG 2 ;
             CAR ;
             SENDER ;
             GET ;
             IF_NONE { PUSH mutez 0 } {} ;
             DUP ;
             DIG 2 ;
             DUP ;
             DUG 3 ;
             COMPARE ;
             GT ;
             IF { PUSH string "AvocadoExchange | A try to withdraw" ; FAILWITH } {} ;
             DIG 2 ;
             DUP ;
             DUG 3 ;
             CDR ;
             DIG 3 ;
             CAR ;
             DIG 3 ;
             DIG 3 ;
             SUB ;
             SOME ;
             SENDER ;
             UPDATE ;
             PAIR ;
             NIL operation ;
             PAIR } } }
