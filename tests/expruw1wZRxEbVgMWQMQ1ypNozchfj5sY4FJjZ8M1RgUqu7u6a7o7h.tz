{ parameter (or (nat %call) (mutez %withdraw)) ;
  storage
    (pair (pair (address %admin) (mutez %balance)) (pair (address %fa12) (int %rate))) ;
  code { DUP ;
         CDR ;
         SWAP ;
         CAR ;
         IF_LEFT
           { NIL operation ;
             DIG 2 ;
             DUP ;
             DUG 3 ;
             { CDR ; CAR } ;
             CONTRACT %mint (pair (address %to) (nat %value)) ;
             { IF_NONE { PUSH unit Unit ; FAILWITH } {} } ;
             PUSH mutez 0 ;
             DIG 3 ;
             DUP ;
             DUG 4 ;
             SELF ;
             ADDRESS ;
             PAIR %to %value ;
             TRANSFER_TOKENS ;
             CONS ;
             SWAP ;
             DROP }
           { SWAP ;
             DUP ;
             DUG 2 ;
             { CAR ; CAR } ;
             SENDER ;
             COMPARE ;
             EQ ;
             IF {}
                { PUSH string "WrongCondition: sp.sender == self.data.admin" ; FAILWITH } ;
             NIL operation ;
             SENDER ;
             CONTRACT unit ;
             { IF_NONE { PUSH unit Unit ; FAILWITH } {} } ;
             DIG 2 ;
             DUP ;
             DUG 3 ;
             PUSH unit Unit ;
             TRANSFER_TOKENS ;
             CONS ;
             DIG 2 ;
             DUP ;
             DUG 3 ;
             DUP ;
             CDR ;
             SWAP ;
             { CAR ; CAR } ;
             DIG 3 ;
             DUP ;
             DUG 4 ;
             DIG 5 ;
             DUP ;
             DUG 6 ;
             { CAR ; CDR } ;
             SUB ;
             SWAP ;
             PAIR ;
             PAIR ;
             DUG 3 ;
             DIG 2 ;
             DROP ;
             SWAP ;
             DROP } ;
         PAIR } }
