{ parameter
    (or (or (or (string %add_candidate) (unit %close_election)) (string %remove_candidate))
        (string %vote)) ;
  storage
    (pair (pair (pair (map %candidates string int) (address %owner)) (map %voters address bool))
          (bool %voting_ended)) ;
  code { DUP ;
         CDR ;
         SWAP ;
         CAR ;
         IF_LEFT
           { IF_LEFT
               { IF_LEFT
                   { PUSH bool True ;
                     DIG 2 ;
                     DUP ;
                     DUG 3 ;
                     CDR ;
                     COMPARE ;
                     EQ ;
                     IF { PUSH string "Voting has already ended" ; FAILWITH } {} ;
                     SWAP ;
                     DUP ;
                     DUG 2 ;
                     { CAR ; CAR ; CAR } ;
                     SWAP ;
                     DUP ;
                     DUG 2 ;
                     MEM ;
                     IF { PUSH string "Candidate already present" ; FAILWITH }
                        { SWAP ;
                          DUP ;
                          DUG 2 ;
                          DUP ;
                          CDR ;
                          SWAP ;
                          CAR ;
                          DUP ;
                          CDR ;
                          SWAP ;
                          CAR ;
                          DUP ;
                          CDR ;
                          SWAP ;
                          CAR ;
                          PUSH (option int) (Some 0) ;
                          DIG 5 ;
                          DUP ;
                          DUG 6 ;
                          UPDATE ;
                          PAIR ;
                          PAIR ;
                          PAIR ;
                          DUG 2 ;
                          SWAP ;
                          DROP } ;
                     DROP }
                   { SENDER ;
                     DIG 2 ;
                     DUP ;
                     DUG 3 ;
                     { CAR ; CAR ; CDR } ;
                     COMPARE ;
                     NEQ ;
                     IF { PUSH string "Only contract owner can close election" ; FAILWITH } {} ;
                     SWAP ;
                     DUP ;
                     DUG 2 ;
                     CAR ;
                     PUSH bool True ;
                     SWAP ;
                     PAIR ;
                     DUG 2 ;
                     DROP ;
                     DROP } }
               { PUSH bool True ;
                 DIG 2 ;
                 DUP ;
                 DUG 3 ;
                 CDR ;
                 COMPARE ;
                 EQ ;
                 IF { PUSH string "Voting has already ended" ; FAILWITH } {} ;
                 SENDER ;
                 DIG 2 ;
                 DUP ;
                 DUG 3 ;
                 { CAR ; CAR ; CDR } ;
                 COMPARE ;
                 NEQ ;
                 IF { PUSH string "Only contract owner can remove candidate" ; FAILWITH } {} ;
                 SWAP ;
                 DUP ;
                 DUG 2 ;
                 { CAR ; CAR ; CAR } ;
                 SWAP ;
                 DUP ;
                 DUG 2 ;
                 MEM ;
                 IF { SWAP ;
                      DUP ;
                      DUG 2 ;
                      DUP ;
                      CDR ;
                      SWAP ;
                      CAR ;
                      DUP ;
                      CDR ;
                      SWAP ;
                      CAR ;
                      DUP ;
                      CDR ;
                      SWAP ;
                      CAR ;
                      NONE int ;
                      DIG 5 ;
                      DUP ;
                      DUG 6 ;
                      UPDATE ;
                      PAIR ;
                      PAIR ;
                      PAIR ;
                      DUG 2 ;
                      SWAP ;
                      DROP }
                    { PUSH string "No candidate" ; FAILWITH } ;
                 DROP } }
           { PUSH bool True ;
             DIG 2 ;
             DUP ;
             DUG 3 ;
             CDR ;
             COMPARE ;
             EQ ;
             IF { PUSH string "Voting has already ended" ; FAILWITH } {} ;
             SWAP ;
             DUP ;
             DUG 2 ;
             { CAR ; CDR } ;
             SENDER ;
             MEM ;
             IF { PUSH string "Voter has already voted" ; FAILWITH }
                { SWAP ;
                  DUP ;
                  DUG 2 ;
                  DUP ;
                  CDR ;
                  SWAP ;
                  CAR ;
                  DUP ;
                  CAR ;
                  SWAP ;
                  CDR ;
                  PUSH (option bool) (Some True) ;
                  SENDER ;
                  UPDATE ;
                  SWAP ;
                  PAIR ;
                  PAIR ;
                  DUG 2 ;
                  SWAP ;
                  DROP } ;
             SWAP ;
             DUP ;
             DUG 2 ;
             { CAR ; CAR ; CAR } ;
             SWAP ;
             DUP ;
             DUG 2 ;
             MEM ;
             IF { SWAP ;
                  DUP ;
                  DUG 2 ;
                  DUP ;
                  CDR ;
                  SWAP ;
                  CAR ;
                  DUP ;
                  CDR ;
                  SWAP ;
                  CAR ;
                  DUP ;
                  CDR ;
                  SWAP ;
                  CAR ;
                  PUSH int 1 ;
                  DIG 6 ;
                  DUP ;
                  DUG 7 ;
                  { CAR ; CAR ; CAR } ;
                  DIG 6 ;
                  DUP ;
                  DUG 7 ;
                  GET ;
                  { IF_NONE { PUSH string "Get-item:43" ; FAILWITH } {} } ;
                  ADD ;
                  SOME ;
                  DIG 5 ;
                  DUP ;
                  DUG 6 ;
                  UPDATE ;
                  PAIR ;
                  PAIR ;
                  PAIR ;
                  DUG 2 ;
                  SWAP ;
                  DROP }
                { PUSH string "Candidate is not valid" ; FAILWITH } ;
             DROP } ;
         NIL operation ;
         PAIR } }
