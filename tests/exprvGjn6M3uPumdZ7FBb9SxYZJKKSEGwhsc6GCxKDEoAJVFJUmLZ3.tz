                {} ;
             SWAP ;
             DUP ;
             DUG 2 ;
             { CDR ; CAR } ;
             SWAP ;
             DUP ;
             DUG 2 ;
             MEM ;
             IF {}
                { PUSH string "WrongCondition: self.data.candidates.contains(params.candidate)" ;
                  FAILWITH } ;
             SWAP ;
             DUP ;
             DUG 2 ;
             DUP ;
             CAR ;
             SWAP ;
             CDR ;
             DUP ;
             CDR ;
             SWAP ;
             CAR ;
             PUSH int 1 ;
             DIG 5 ;
             DUP ;
             DUG 6 ;
             { CDR ; CAR } ;
             DIG 5 ;
             DUP ;
             DUG 6 ;
             GET ;
             { IF_NONE { PUSH string "Get-item:17" ; FAILWITH } {} } ;
             ADD ;
             SOME ;
             DIG 4 ;
             DUP ;
             DUG 5 ;
             UPDATE ;
             PAIR ;
             SWAP ;
             PAIR ;
             DUG 2 ;
             SWAP ;
             DROP ;
