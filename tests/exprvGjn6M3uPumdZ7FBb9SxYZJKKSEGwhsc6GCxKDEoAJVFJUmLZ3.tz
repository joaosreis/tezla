             DUP ;
             DUG 2 ;
             DUP ;
             CAR ;
             SWAP ;
             CDR ;
             DUP ;
             CAR ;
             SWAP ;
             CDR ;
             PUSH (option int) (Some 1) ;
             SENDER ;
             UPDATE ;
             SWAP ;
             PAIR ;
             SWAP ;
             PAIR ;
             DUG 2 ;
             DROP 2 }
           { SWAP ;
             DUP ;
             DUG 2 ;
             CAR ;
             SENDER ;
             COMPARE ;
             EQ ;
             IF {}
                { PUSH string "WrongCondition: sp.sender == self.data.admin" ; FAILWITH } ;
             SWAP ;
             DUP ;
             DUG 2 ;
             { CDR ; CAR } ;
             SWAP ;
             DUP ;
             DUG 2 ;
             MEM ;
             IF { PUSH string "WrongCondition: ~ (self.data.candidates.contains(params.candidate))" ;
                  FAILWITH }
                {} ;
             SWAP ;
             DUP ;
             DUG 2 ;
             DUP ;
             CAR ;
             SWAP ;
             CDR ;
             DUP ;
             CDR ;
             SWAP ;
             CAR ;
             PUSH (option int) (Some 0) ;
             DIG 4 ;
             DUP ;
             DUG 5 ;
             UPDATE ;
             PAIR ;
             SWAP ;
             PAIR ;
             DUG 2 ;
             DROP 2 } ;
         NIL operation ;
         PAIR } }
